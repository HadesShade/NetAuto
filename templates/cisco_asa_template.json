#JSON Configuration template for cisco_ios. You can add multiple devices inside the "devices" section.
#Each iteration (multiple list items, multiple JSON items, and etc) are separated by commas (,) as JSON standard.
#Remove these comment sections to use this template as configuration file.

{
	"devices" : [
			{
				"model" : "cisco_asa",
				"remote-protocol" : "<protocol here>",
				"ip" : "<ip here>",
				"port" : "<port here>",
				"username" : "<optional if used>",
				"password" : "<optional if used>",
				"secret" : "<optional if used>",
				"verbose" : "<optional - either true or false. Default is false>",
				"user_exec" : [
						"user_exec_commands_here_with_comma_separator_for_each_command"
					],
				"priv_exec" : [
						"privileged_exec_commands_here_with_comma_separator_for_each_command"
					],
				"global_config" : [
						"global_config_commands_here_with_comma_separator_for_each_command"
					]
			}
	]
}


